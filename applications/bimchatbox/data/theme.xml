<?xml version="1.0" encoding="UTF-8"?>
<theme name="Default" author_name="Invision Power Services, Inc" author_url="https://www.invisioncommunity.com">
 <template template_group="chat" template_name="chatform" template_data="$chats, $orientation" template_location="front" template_app="bimchatbox"><![CDATA[<div class='chatboxMedia ipsAreaBackground_light'></div>
<div class='ipsAreaBackground_light ipsPadding:half' id='chatBoxForm'>
	<div class="bimcb_chatArea ipsBox--child ipsClearfix">
		{{if \IPS\Application::load('bimchatbox')->can_Chat()}}
			{{if !\in_array(member.member_id, explode(",", \IPS\Settings::i()->chatbox_conf_blocklist))}}
				<form data-action='chatform' id="cbInput_container">
					<input class='bimcb_chatInput' id="txt" name="txt" placeholder='{lang="chatbox_placeholder"}' autocomplete="off">
					<div class='cbItems'>
						<a href='#' data-ipsmenu data-ipsmenu-above='true' data-ipsmenu-appendto='#ipsLayout_contentArea' data-ipsmenu-closeonclick='false' id='elEmoticons_chatbox' data-ipstooltip title='{lang="chatbox_emoticons"}'>
							<img src='{resource="emo.png" app="bimchatbox" location="front"}' width='24px' height='24px'>
						</a>
						{{if \IPS\Settings::i()->chatbox_conf_giphy == "button" || \IPS\Settings::i()->chatbox_conf_giphy == "both"}}
							<a href='#' class="bimGiphyBtn" data-action="bimGiphy" data-ipstooltip title='{lang="bim_giphy"}'>
								<img src='{resource="bimgiphy.png" app="bimchatbox" location="front"}' width='24px' height='24px'>
							</a>
						{{endif}}					
						<button type="submit" class="ipsButton ipsButton_primary ipsButton_verySmall {{if \IPS\Settings::i()->chatbox_conf_hide_enter_mobile == 1}}ipsResponsive_hidePhone{{endif}}" data-action="chat" id="chat_button">
							<span class="ipsType_small"><i class="fa fa-level-down fa-rotate-90"></i></span>
						</button>
					</div>
				</form>
			{{else}}
				<div class='ipsPadding:half ipsType_warning'>
					<i class="fa fa-exclamation-triangle"></i> {lang="chatbox_error_inblock"}
				</div>
			{{endif}}
		{{else}}
			<div class='ipsPadding:half ipsType_warning'>
				<i class="fa fa-exclamation-triangle"></i> {lang="chatbox_error_noper"}
			</div>
		{{endif}}
	</div>
</div>]]></template>
 <template template_group="chat" template_name="dashboardHeader" template_data="" template_location="admin" template_app="bimchatbox"></template>
 <template template_group="chat" template_name="edit" template_data="$chat" template_location="front" template_app="bimchatbox"><![CDATA[<div class='ipsBox' data-controller="bimChatBoxMain">
	<h3 class="ipsType_reset ipsType_sectionTitle ipsClearfix">{lang="chatbox_edit"}</h3>
	<div class="ipsPadding">
		<textarea rows="4" cols="50" id="editmsg_{$chat['id']}">{$chat['chat']}</textarea>
		<br><br>
		<a href="#" data-action="edit" data-id="{$chat['id']}" class="ipsButton ipsButton_primary">{lang="save"}</a>		
	</div>
</div>]]></template>
 <template template_group="chat" template_name="emoticons" template_data="$emoticons" template_location="front" template_app="bimchatbox"><![CDATA[<div data-controller="bimChatBoxMain">
	<div id='chatbox_emoticonMenu'>
		<div class='ipsMenu_headerBar'>
			<p class='ipsType_reset ipsPos_right'><a href='#' class='ipsType_blendLinks' data-role='categoryTrigger' data-ipsMenu data-ipsMenu-appendTo='#chatbox_emoticonMenu' id='id_more'>{lang="chatbox_emocategories"} <i class='fa fa-caret-down'></i></a></p>
			<h4 class='ipsType_sectionHead'>{lang="chatbox_emoticons"}</h4>
			<ul data-role='categoryMenu' class='ipsMenu ipsMenu_auto' id='id_more_menu' role='menu' style='display: none'>
				{{foreach $emoticons as $k => $group}}
					<li class='ipsMenu_item' role='menuitem' data-ipsMenuValue='{$k}' data-action='EmoCatChoose'><a href='#'>{$group['title']}</a></li>
				{{endforeach}}
			</ul>
		</div>
		<div class='ipsMenu_innerContent ipsEmoticons_content'>
			{{foreach $emoticons as $k => $group}}
				{{$cnt++;}}
				<div id='emoCat_{$k}' class='emoCat' {{if $cnt > 1}}style="display: none;"{{endif}}>
					<div class='ipsAreaBackground_light ipsPadding:half'><strong>{$group['title']}</strong></div>
					<div class='ipsEmoticons_category' data-categoryid='{$k}'>
						{{foreach (array) $group['emoticons'] as $emo}}
							<div data-action="emo_emo" class="cbEmoList" data-emoticon="{$emo['text']}" title="{$emo['text']}"><img src="{$emo['src']}"></div>
						{{endforeach}}				
					</div>			
				</div>
			{{endforeach}}
		</div>
	</div>
</div>]]></template>
 <template template_group="chat" template_name="main" template_data="$orientation" template_location="front" template_app="bimchatbox"><![CDATA[
{{$ann = (string) \IPS\Settings::i()->chatbox_conf_announcements;}}

<div data-controller="bim.chatbox.free">
	<h2 class="ipsType_sectionTitle ipsType_reset" id="elChatbox" >
		<ul class="ipsPos_right ipsList_inline ipsList_noSpacing">
			<li>
				<a href="#" class="cbBtn" data-action="toggleSound" data-ipstooltip="" title='{lang="chatbox_togglesound"}'>
					<i class="fa fa-volume-up"></i>
				</a>
			</li>
			{{if \IPS\Application::load('bimchatbox')->can_Manage()}}
				<li>
					<a href="#" class="cbBtn" data-ipsDialog data-ipsDialog-forceReload data-ipsDialog-url='{url="app=bimchatbox&module=chatbox&controller=chatbox&do=config"}' data-ipstooltip="" title='{lang="chatbox_management"}'>
						<i class="fa fa-cog"></i>
					</a>			
				</li>
			{{endif}}		
		</ul>
		<div>{lang="chatbox_title"}</div>	
	</h2>
	
	<div id="elChatboxContent">
		{{if $ann}}
			<div class="cbAnn ipsPadding:half ipsClearfix">
				{{if \IPS\Settings::i()->chatbox_conf_anntab == 1}}
					<a href="#" data-ipsDialog data-ipsDialog-forceReload data-ipsDialog-title='{lang="chatbox_rules"}' data-ipsDialog-url='{url="app=bimchatbox&module=chatbox&controller=chatbox&do=chatrules"}' data-ipsDialog-size='narrow'>
						<i class="fa fa-bullhorn" aria-hidden="true"></i>&nbsp;&nbsp;{lang="chatbox_rules_link"}
					</a>
				{{else}}
					{$ann|raw}
				{{endif}}
			</div>
		{{endif}}
		
		{{if \IPS\Settings::i()->chatbox_conf_ordertop == 1}}
			{template="chatform" app="bimchatbox" group="chat" params="$chat, $orientation"}
		{{endif}}			
		
		<div id='chatboxWrap' {{if \IPS\Settings::i()->chatbox_conf_height}}style='height: {setting="chatbox_conf_height"}px !important;'{{endif}}>
			{{if \IPS\Settings::i()->chatbox_conf_ordertop != 1}}
				<div id='loadMore' data-action='loadMore' class='ipsAreaBackground_light ipsPadding:half ipsType_center ipsCursor_pointer'>{lang="cb_loadmore"}</div>
			{{endif}}
			
			<ul id='chatcontent' class="ipsDataList ipsDataList_reducedSpacing ipsLoading">
			
			</ul>
			
			{{if \IPS\Settings::i()->chatbox_conf_ordertop == 1}}
				<div id='loadMore' data-action='loadMore' class='ipsAreaBackground_light ipsPadding:half ipsType_center ipsCursor_pointer'>{lang="cb_loadmore"}</div>
			{{endif}}				
		</div>
		
		{{if \IPS\Settings::i()->chatbox_conf_ordertop != 1}}
			{template="chatform" app="bimchatbox" group="chat" params="$chat, $orientation"}
		{{endif}}
	</div>
</div>]]></template>
 <template template_group="chat" template_name="manage" template_data="$form" template_location="front" template_app="bimchatbox"><![CDATA[<h3 class="ipsType_reset ipsType_sectionTitle ipsClearfix">{lang="chatbox_management"}</h3>
<div class="ipsPadding chatboxConf" data-controller="bim.chatbox.manage">
	{$form|raw}
</div>]]></template>
 <template template_group="chat" template_name="rules" template_data="$txt" template_location="front" template_app="bimchatbox"><![CDATA[<div class="ipsPadding chatboxConf" data-controller="bim.chatbox.manage">
	{$txt|raw}
</div>]]></template>
 <template template_group="chat" template_name="video" template_data="$source, $id" template_location="front" template_app="bimchatbox"><![CDATA[<div class="cbVideoWrapper">
	{{if $source == 'youtube'}}
		<iframe width="560" height="315" src="https://www.youtube.com/embed/{$id}?autoplay=1" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
	{{endif}}
</div>]]></template>
 <template template_group="embed" template_name="chatvars" template_data="$emoticons, $badwords" template_location="front" template_app="bimchatbox"><![CDATA[<script type='text/javascript'>
	ips.setSetting( 'chatbox_conf_interval', {setting="chatbox_conf_interval"} );
	ips.setSetting( 'chatbox_soundEnabled', ips.utils.db.get( 'chatbox', 'sounds' ) );
	ips.setSetting( 'chatbox_topStyle', {setting="chatbox_conf_ordertop"} );	
	ips.setSetting( 'chatbox_maxMSG', {setting="chatbox_conf_chatlimit"} );
	ips.setSetting( 'chatbox_maxEmoticons', {setting="chatbox_conf_maxemoticons"} );	
	ips.setSetting( 'chatbox_Emoticons', {expression="json_encode( $emoticons )" raw="true"} );	
	ips.setSetting( 'badwords', 
		{expression="json_encode( $badwords )" raw="true"}
	);	
	ips.setSetting( 'chatbox_imgPost', {setting="chatbox_conf_imgPost"} );	
	ips.setSetting( 'chatbox_videoPost', {setting="chatbox_conf_videoPost"} );	
	ips.setSetting( 'chatbox_giphy', '{setting="chatbox_conf_giphy"}' );	
	ips.setSetting( 'chatbox_24h', {{if \IPS\Settings::i()->chatbox_conf_timeformat == '24'}}true{{else}}false{{endif}} );	
	ips.setSetting( 'chatbox_getAll', false );
	{{if \IPS\Member::loggedin()->member_id}}
		ips.setSetting( 'chatbox_myname', '{expression="\IPS\Member::loggedin()->name"}' );
	{{endif}}
	ips.setSetting( 'chatbox_version', '{expression="\IPS\Application::load('bimchatbox')->version"}' );	
</script>]]></template>
 <template template_group="widgets" template_name="bimchatbox" template_data="$orientation='vertical'" template_location="front" template_app="bimchatbox">{template=&quot;main&quot; group=&quot;chat&quot; app=&quot;bimchatbox&quot; params=&quot;$orientation&quot;}</template>
